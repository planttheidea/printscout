{
  "author": "planttheidea",
  "bugs": {
    "url": "https://github.com/planttheidea/printscout/issues"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^3.1.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.4.1",
    "html-webpack-plugin": "^2.22.0",
    "in-publish": "^2.0.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "sinon": "^1.17.4",
    "tape": "^4.6.0",
    "tape-run": "^2.1.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "description": "Handle print events with ease",
  "homepage": "https://github.com/planttheidea/printscout#readme",
  "keywords": [
    "print",
    "beforeprint",
    "afterprint",
    "matchMedia",
    "print",
    "events"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "printscout",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/printscout.git"
  },
  "scripts": {
    "build": "NODE_ENV=development ./node_modules/webpack/bin/webpack.js --progress --colors",
    "build-minified": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --progress --colors --config=webpack.config.minified.js",
    "compile-for-publish": "npm run lint && npm run test && npm run transpile && npm run dist",
    "dev": "NODE_ENV=development ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors --config=webpack.config.dev.js",
    "dist": "npm run build && npm run build-minified",
    "lint": "NODE_ENV=test ./node_modules/eslint/bin/eslint.js src",
    "prepublish": "in-publish && npm run compile-for-publish || in-install",
    "test": "NODE_ENV=test browserify -t babelify test/index.js | tape-run",
    "transpile": "./node_modules/babel-cli/lib/babel/index.js -d lib/ src/"
  },
  "version": "1.1.3"
}
