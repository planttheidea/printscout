{
  "author": "planttheidea",
  "bugs": {
    "url": "https://github.com/planttheidea/printscout/issues"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.22.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-polyfill": "6.22.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "babel-register": "6.22.0",
    "babelify": "7.3.0",
    "browserify": "13.3.0",
    "eslint": "3.14.0",
    "eslint-friendly-formatter": "2.0.7",
    "eslint-loader": "1.6.1",
    "html-webpack-plugin": "2.26.0",
    "in-publish": "2.0.0",
    "optimize-js-plugin": "0.0.4",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "sinon": "1.17.7",
    "tape": "4.6.3",
    "tape-run": "2.1.5",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "description": "Handle print events with ease",
  "homepage": "https://github.com/planttheidea/printscout#readme",
  "keywords": [
    "print",
    "beforeprint",
    "afterprint",
    "matchMedia",
    "print",
    "events"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "printscout",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/printscout.git"
  },
  "scripts": {
    "build": "NODE_ENV=development ./node_modules/webpack/bin/webpack.js --progress --colors",
    "build:minified": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --progress --colors --config=webpack.config.minified.js",
    "dev": "NODE_ENV=development ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors --config=webpack.config.dev.js",
    "dist": "npm run build && npm run build:minified",
    "lint": "NODE_ENV=test ./node_modules/eslint/bin/eslint.js src",
    "prepublish": "in-publish && npm run prepublish:compile || echo",
    "prepublish:compile": "npm run lint && npm run test && npm run transpile && npm run dist",
    "test": "NODE_ENV=test browserify -t babelify test/index.js | tape-run",
    "transpile": "babel src --out-dir lib"
  },
  "version": "1.1.4"
}
